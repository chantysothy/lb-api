{
  "name": "Friends",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "status": {
      "type": "number",
      "required": true,
      "enum": [1,2,3]
    },
    "createdAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "_users": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_userId"
    },
    "_friends": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_friendId"
    }
  },
  "acls": [],
  "methods": {}
}
