{
  "name": "Notifications",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "messageType": {
      "type": "number",
      "required": true,
      "enum": [
        1,
        2,
        3
      ]
    },
    "createdAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "_receivers": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_receiverId"
    },
    "_senders": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_senderId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {},
  "mixins": {
    "SoftDelete" : true
  }
}
