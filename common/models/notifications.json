{
  "name": "Notifications",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "messageType": {
      "type": "number",
      "required": true,
      "enum": [1,2,3]
    }
  },
  "validations": [],
  "relations": {
    "_receivers": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_receiverId"
    },
    "_senders": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_senderId"
    }
  },
  "acls": [],
  "methods": {}
}
