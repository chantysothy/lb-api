{
  "name": "Posts",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
  	"postTitle": {
  		"type": "string",
  		"required": true,
      	"index": true
  	},
  	"postContent": {
  		"type": "string",
      	"index": true
    },
    "createdAt": {
  		"type": "date"
    },
    "updatedAt": {
  		"type": "date"
    },
    "deletedAt": {
  		"type": "date"
    }
  },
  "validations": [],
  "relations": {
  	"_users": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "_userId"
    },
    "_photos": {
      "type": "belongsTo",
      "model": "Photos",
      "foreignKey": "_photoId"
    },
    "_videos": {
      "type": "belongsTo",
      "model": "Videos",
      "foreignKey": "_videoId"
    }
  },
  "acls": [],
  "methods": {}
}